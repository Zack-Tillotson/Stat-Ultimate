<h2>Points</h2>
<div class="graph" id="point-graph-player-<%= player.id %>"><img src="/spinner.gif"></div>
<h2>Errors</h2>
<div class="graph" id="error-graph-player-<%= player.id %>"><img src="/spinner.gif"></div>
<script type="text/javascript">
  $.getJSON('<%= pointgraph_player_path(player, :format => :json, :container => "point-graph-player-#{player.id}") %>', function(data) {new Highcharts.Chart(data)})
  $.getJSON('<%= errorgraph_player_path(player, :format => :json, :container => "error-graph-player-#{player.id}") %>', function(data) {new Highcharts.Chart(data)})
</script>
